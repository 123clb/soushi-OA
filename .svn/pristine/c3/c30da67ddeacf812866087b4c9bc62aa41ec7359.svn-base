<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.kaoshidian.oa.assets.entity">
    <class name="DeviceCategory" table="oa_assets_devicecategory">
        <id column="categoryId" type="java.lang.Integer" name="categoryId">
            <generator class="identity"></generator>
        </id>
        <property name="categoryName" type="string"  column="categoryName"></property>
        <property name="createDate" type="java.util.Date"  column="createDate"></property>
        <property name="description" type="string"  column="description"></property>        
    </class>
</hibernate-mapping>