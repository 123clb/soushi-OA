package com.kaoshidian.oa.base.exception;

/**
 * 当从视频服务器获取视频信息时出现错误，或者视频服务器返回错误消息，可以考虑抛出此异常
 * @author <p>Innate Solitary 于 2012-5-8 上午10:59:47</p>
 *
 */
public class KsdGetVideoInfoException extends KsdException {

	/**
     * 
     */
    private static final long serialVersionUID = 2488301125804137279L;

	public KsdGetVideoInfoException() {
	    super();
    }

	public KsdGetVideoInfoException(String message, Throwable cause) {
	    super(message, cause);
    }

	public KsdGetVideoInfoException(String message) {
	    super(message);
    }

	public KsdGetVideoInfoException(Throwable cause) {
	    super(cause);
    }
    
}
