package com.kaoshidian.oa.attend.entity;

import java.util.Date;

import com.kaoshidian.oa.base.entity.BaseEntity;
/**
 * @author <p>123clb 于 2013-5-21 下午6:59:39</p>
 *
 */
public class AttendData extends BaseEntity {
		
	private Integer attendDataId;//数据ID
	
	private String workerId;//打卡人ID
	
	private String  workerName;//打卡人姓名
	
	private Date  workTime;//打卡时间


	public Integer getAttendDataId() {
		return attendDataId;
	}

	public void setAttendDataId(Integer attendDataId) {
		this.attendDataId = attendDataId;
	}

	public String getWorkerId() {
		return workerId;
	}

	public void setWorkerId(String workerId) {
		this.workerId = workerId;
	}

	public String getWorkerName() {
		return workerName;
	}

	public void setWorkerName(String workerName) {
		this.workerName = workerName;
	}

	public Date getWorkTime() {
		return workTime;
	}

	public void setWorkTime(Date workTime) {
		this.workTime = workTime;
	}

	
	
}
