package com.kaoshidian.oa.base.exception;

/**
 * 考试点checked根异常
 * @author <p>Innate Solitary 于 2012-5-8 上午10:57:16</p>
 *
 */
public class KsdException extends Exception {

	/**
     * 
     */
    private static final long serialVersionUID = -5874723351037847711L;

	public KsdException() {
	    super();
    }

	public KsdException(String message, Throwable cause) {
	    super(message, cause);
    }

	public KsdException(String message) {
	    super(message);
    }

	public KsdException(Throwable cause) {
	    super(cause);
    }
    
    

}
