package com.kaoshidian.oa.assets.entity;

import java.util.Date;

import com.kaoshidian.oa.base.entity.BaseEntity;
import com.kaoshidian.oa.util.DeviceStateEnum;

/**
 * @author <p>123clb 于 2013-5-7 下午12:19:41</p>
 *
 */
public class Device extends BaseEntity {
	
		private Integer deviceId;//设备ID
		private String  deviceName;//设备名称
		private Integer categoryId; //设备类型
		private String  brand;//品牌
		private String  dNumber;//型号
		private String  sNumber;//序列号
		private Integer supplierID;//供应商
		private Date   updateDate;//更新时间
		private Date scrapDate;//报废时间
		private Double  devicePrice;//设备价格
		private DeviceStateEnum deviceState;//使用状态
		private  String attaChment;//附件
		private  Integer supervisePerson;//监管人
		private  Integer createPerson;//创建者
		private String usePersonName;//使用者
		private Integer usePersonId;//使用者ID
		private Date purchaseDate;
		private String devicePic;//设备图片
		private String scrapPic;//报废图片
		
		public Integer getDeviceId() {
			return deviceId;
		}
		public void setDeviceId(Integer deviceId) {
			this.deviceId = deviceId;
		}
		public String getDeviceName() {
			return deviceName;
		}
		public void setDeviceName(String deviceName) {
			this.deviceName = deviceName;
		}
		public String getBrand() {
			return brand;
		}
		public void setBrand(String brand) {
			this.brand = brand;
		}
		public String getdNumber() {
			return dNumber;
		}
		public void setdNumber(String dNumber) {
			this.dNumber = dNumber;
		}
		public String getsNumber() {
			return sNumber;
		}
		public void setsNumber(String sNumber) {
			this.sNumber = sNumber;
		}
		public Integer getSupplierID() {
			return supplierID;
		}
		public void setSupplierID(Integer supplierID) {
			this.supplierID = supplierID;
		}
		public Date getUpdateDate() {
			return updateDate;
		}
		public void setUpdateDate(Date updateDate) {
			this.updateDate = updateDate;
		}
		public Date getScrapDate() {
			return scrapDate;
		}
		public void setScrapDate(Date scrapDate) {
			this.scrapDate = scrapDate;
		}
		public Double getDevicePrice() {
			return devicePrice;
		}
		public void setDevicePrice(Double devicePrice)
		{
			this.devicePrice = devicePrice;
		}
		public DeviceStateEnum getDeviceState() {
			return deviceState;
		}
		public void setDeviceState(DeviceStateEnum deviceState) {
			this.deviceState = deviceState;
		}
		public String getAttaChment() {
			return attaChment;
		}
		public void setAttaChment(String attaChment) {
			this.attaChment = attaChment;
		}
		public Integer getSupervisePerson() {
			return supervisePerson;
		}
		public void setSupervisePerson(Integer supervisePerson) {
			this.supervisePerson = supervisePerson;
		}
		public Integer getCategoryId() {
			return categoryId;
		}
		public void setCategoryId(Integer categoryId) {
			this.categoryId = categoryId;
		}
		public Integer getCreatePerson() {
			return createPerson;
		}
		public void setCreatePerson(Integer createPerson) {
			this.createPerson = createPerson;
		}
		
		public Date getPurchaseDate() {
			return purchaseDate;
		}
		public void setPurchaseDate(Date purchaseDate) {
			this.purchaseDate = purchaseDate;
		}
		public String getDevicePic() {
			return devicePic;
		}
		public void setDevicePic(String devicePic) {
			this.devicePic = devicePic;
		}
		
		public String getUsePersonName() {
			return usePersonName;
		}
		public void setUsePersonName(String usePersonName) {
			this.usePersonName = usePersonName;
		}
		public Integer getUsePersonId() {
			return usePersonId;
		}
		public void setUsePersonId(Integer usePersonId) {
			this.usePersonId = usePersonId;
		}
		public String getScrapPic() {
			return scrapPic;
		}
		public void setScrapPic(String scrapPic) {
			this.scrapPic = scrapPic;
		}
		
		
