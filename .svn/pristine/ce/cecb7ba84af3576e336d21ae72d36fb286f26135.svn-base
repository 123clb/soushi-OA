package com.kaoshidian.oa.util;

/**
 * @author <p>Innate Solitary 于 2012-7-2 上午11:49:26</p>
 *
 */
public final class SystemConfig {
	
	private String symmetricKey;
	private String frontroot;
	private String memberroot;
	private Boolean rtxCanSend;
	private String uploadDir;
	public String getSymmetricKey() {
    	return symmetricKey;
    }
	
	public void setSymmetricKey(String symmetricKey) {
    	this.symmetricKey = symmetricKey;
    }

	public String getFrontroot() {
	    return frontroot;
    }

	public void setFrontroot(String frontroot) {
	    this.frontroot = frontroot;
    }

	public String getMemberroot() {
	    return memberroot;
    }

	public void setMemberroot(String memberroot) {
	    this.memberroot = memberroot;
    }

	public Boolean getRtxCanSend() {
	    return rtxCanSend;
    }

	public void setRtxCanSend(Boolean rtxCanSend) {
	    this.rtxCanSend = rtxCanSend;
    }

	public String getUploadDir() {
		return uploadDir;
	}

	public void setUploadDir(String uploadDir) {
		this.uploadDir = uploadDir;
	}

	
	
}
