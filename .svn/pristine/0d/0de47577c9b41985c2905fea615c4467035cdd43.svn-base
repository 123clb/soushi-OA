package com.kaoshidian.oa.base.exception;

/**
 * FTP客户端上传失败时抛出此异常
 * @author <p>Innate Solitary 于 2012-2-29 下午12:05:21</p>
 *
 */
public class FtpUploadFailureException extends KsdBizSystemErrorException {

	/**
     * 
     */
    private static final long serialVersionUID = -2580833086368344018L;

	public FtpUploadFailureException() {
	    super();
    }

	public FtpUploadFailureException(String message, Throwable cause) {
	    super(message, cause);
    }

	public FtpUploadFailureException(String msg) {
	    super(msg);
    }

	public FtpUploadFailureException(Throwable cause) {
	    super(cause);
    }
    
    

}
