<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.kaoshidian.oa.permission.entity">
    <class name="Role" table="oa_perm_role">
        <id column="roleId" type="java.lang.Integer" name="roleId">
            <generator class="identity"></generator>
        </id>
        
        <property name="roleName" type="string" column="roleName"></property>
        <property name="label" type="string" column="label"></property>
        <property name="createDate" type="java.util.Date" column="createDate"></property>
        <property name="description" type="string" column="description"></property>
    </class>
</hibernate-mapping>