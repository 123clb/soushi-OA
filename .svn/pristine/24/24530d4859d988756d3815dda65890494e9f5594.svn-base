package com.kaoshidian.oa.attend.entity;
import com.kaoshidian.oa.base.entity.BaseEntity;
public class AttendInfo extends BaseEntity {
	
	private Integer infoId;
	private String  rtxNo;//rtxNo 
	private String personName;//统计姓名
	private Integer yearNum;//年
	private Integer  monthNum;//月份
	private String  infoTime;//
	private Integer laterNum;//迟到次数
	private Double  leaveNum; //请假天数
	
	public String getPersonName() {
		return personName;
	}
	public void setPersonName(String personName) {
		this.personName = personName;
	}
	public Integer getMonthNum() {
		return monthNum;
	}
	public void setMonthNum(Integer monthNum) {
		this.monthNum = monthNum;
	}
	public Integer getLaterNum() {
		return laterNum;
	}
	public void setLaterNum(Integer laterNum) {
		this.laterNum = laterNum;
	}
	public Double getLeaveNum() {
		return leaveNum;
	}
	public void setLeaveNum(Double leaveNum) {
		this.leaveNum = leaveNum;
	}
	public Integer getInfoId() {
		return infoId;
	}
	public void setInfoId(Integer infoId) {
		this.infoId = infoId;
	}
	public Integer getYearNum() {
		return yearNum;
	}
	public void setYearNum(Integer yearNum) {
		this.yearNum = yearNum;
	}
	public String getRtxNo() {
		return rtxNo;
	}
	public void setRtxNo(String rtxNo) {
		this.rtxNo = rtxNo;
	}
	public String getInfoTime() {
		return infoTime;
	}
	public void setInfoTime(String infoTime) {
		this.infoTime = infoTime;
	}

}
